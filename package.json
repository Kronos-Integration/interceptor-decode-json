{
  "name": "kronos-interceptor-decode-json",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/decode-json.js",
  "module": "src/decode-json.js",
  "description": "decodes json streams into objects",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/decode-json.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/decode-json.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "kronos-interceptor": "^2.5.2",
    "parse-concat-stream": "^0.2.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "kronos-test-interceptor": "^3.1.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.1",
    "rollup": "^0.65.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.9.12",
    "documentation": "^8.1.2",
    "travis-deploy-once": "^5.0.4",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-cleanup": "^3.0.0"
  },
  "engines": {
    "node": ">=10.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-decode-json.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-decode-json/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-decode-json#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}

{
  "name": "@kronos-integration/interceptor-decode-json",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/decode-json.mjs",
  "module": "src/decode-json.mjs",
  "description": "decodes json streams into objects",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/decode-json.mjs --section=API",
    "lint": "documentation lint src/decode-json.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^8.1.0"
  },
  "devDependencies": {
    "@kronos-integration/test-interceptor": "^7.0.0",
    "ava": "^2.4.0",
    "c8": "^7.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "semantic-release": "^17.0.0"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-decode-json.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-decode-json/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-decode-json#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }

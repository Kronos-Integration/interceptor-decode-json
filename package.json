{
  "name": "@kronos-integration/interceptor-decode-json",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/decode-json.mjs"
  },
  "description": "decodes json streams into objects",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },

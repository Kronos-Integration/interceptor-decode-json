{
  "name": "kronos-interceptor-decode-json",
  "version": "0.0.0-semantic-release",
  "description": "decodes json streams into objects",
  "main": "dist/decode-json.js",
  "module": "src/decode-json.js",
  "keywords": [
    "kronos-interceptor"
  ],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/decode-json.js -c -- src/decode-json.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-decode-json.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-decode-json/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-decode-json#readme",
  "devDependencies": {
    "chai": "^4.1.1",
    "jsdoc-to-markdown": "^3.0.0",
    "kronos-test-interceptor": "^2.1.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "rollup": "^0.45.2",
    "semantic-release": "^6.3.6"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.2.1"
  },
  "dependencies": {
    "kronos-interceptor": "2.4.16",
    "parse-concat-stream": "^0.2.0"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}

{
  "name": "@kronos-integration/interceptor-decode-json",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/decode-json.mjs",
  "exports": {
    ".": "./src/decode-json.mjs"
  },
  "description": "decodes json streams into objects",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/decode-json.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/decode-json.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.0.11"
  },
  "devDependencies": {
    "@kronos-integration/test-interceptor": "^7.0.3",
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "^13.1.0",
    "semantic-release": "^17.2.2"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-decode-json.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-decode-json/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-decode-json#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-kronos-component",
      "arlac77/template-github"
    ]
  }
}
